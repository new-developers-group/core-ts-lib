{
  "name": "@new-developers-group/core-ts-lib",
  "version": "0.0.3",
  "description": "re useful classes for typescript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsup lib/index.ts --format cjs,esm --dts",
    "clean": "rm -fdr ./dist && rm -fdr types",
    "test-inband": "NODE_ENV=test jest --runInBand --logHeapUsage",
    "test": "NODE_ENV=test jest --maxWorkers=50%",
    "ci": "npm run lint:ci && npm run test && npm run build",
    "lint:ci": "npm run lint:check && npm run prettier:check",
    "lint:check": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "prettier:check": "prettier --check './**/*.{js,json,ts,tsx,md,yml,yaml}' ",
    "prettier:fix": "prettier --write './**/*.{js,json,ts,tsx,md,yml,yaml}'"
  },
  "repository": {
    "type": "git",
    "url": "github.com?owner=new-developers-group&repo=gp-professional-onboarding-core"
  },
  "author": "new-developers-group/gilbertopsantosjr",
  "dependencies": {
    "axios": "^1.4.0",
    "date-fns": "^2.30.0"
  },
  "peerDependencies": {
    "axios": "^1.4.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "jest-html-reporter": "^3.7.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tscpaths": "^0.0.9",
    "tsup": "^7.2.0",
    "typescript": "^4.5.5"
  }
}
